    "through2": "^3.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "is-promise": "^2.1.0",
    "ora": "^4.0.3",
    "sonic-boom": "^1.0.1"
  },
  "engines": {
    "node": ">=12.4.0"
  },
