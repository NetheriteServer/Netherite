{
  "name": "spile",
  "version": "0.0.1",
  "description": "A Minecraft Server written in TypeScript.",
  "main": "./dist/cli.js",
  "scripts": {
    "dev": "yarn dev:server",
    "dev:server": "ts-node -r module-alias/register --files ./src/cli",
    "test": "tsc --noEmit && eslint src/**/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zorbyte/Spile.git"
  },
  "keywords": [
    "minecraft",
    "server",
    "typescript",
    "threaded"
  ],
  "author": "zorbyte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zorbyte/Spile/issues"
  },
  "homepage": "https://github.com/zorbyte/Spile#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^13.11.1",
    "@types/sonic-boom": "^0.7.0",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-bamboo": "^3.0.0",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "module-alias": "^2.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.9.0-dev.20200416"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "fast-json-stringify": "^2.0.0",
    "flatstr": "^1.0.12",
    "fs-nextra": "^0.4.7",
    "ora": "^4.0.3",
    "ow": "^0.17.0",
    "secure-json-parse": "^2.1.0",
    "sonic-boom": "^1.0.1",
    "typedi": "^0.8.0",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "cpu": [
    "x64",
    "x86"
  ],
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "_moduleAliases": {
    "@root": "./src",
    "@lib": "./src/lib",
    "@utils": "./src/lib/utils",
    "@net": "./src/net",
    "@marshal": "./src/marshal",
    "@_types": "./src/lib/types"
  }
}
