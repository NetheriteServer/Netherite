  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/SpileMC/Spile/issues"
  },
  "homepage": "https://github.com/SpileMC/Spile#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.11.1",
    "@types/sonic-boom": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "cross-env": "^7.0.2",
    "discord.js": "^12.2.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.6.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
